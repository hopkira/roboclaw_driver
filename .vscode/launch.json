{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug RoboClaw Driver",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/ros/a_ws/install/roboclaw_driver/lib/roboclaw_driver/roboclaw_driver_node",
            "args": [
                "--ros-args",
                "--params-file", "${workspaceFolder}/config/motor_driver.yaml",
                "--log-level", "DEBUG"
            ],
            "stopAtEntry": false,
            "cwd": "/home/ros/a_ws",
            "environment": [
                {
                    "name": "ROS_VERSION",
                    "value": "2"
                },
                {
                    "name": "ROS_PYTHON_VERSION",
                    "value": "3"
                },
                {
                    "name": "ROS_DISTRO",
                    "value": "jazzy"
                },
                {
                    "name": "ROS_DOMAIN_ID",
                    "value": "0"
                },
                {
                    "name": "ROS_AUTOMATIC_DISCOVERY_RANGE",
                    "value": "SUBNET"
                },
                {
                    "name": "AMENT_PREFIX_PATH",
                    "value": "/home/ros/a_ws/install/roboclaw_driver:/home/ros/sigyn_testicle_twister_ws/install/sigyn_testicle_twister:/opt/ros/jazzy"
                },
                {
                    "name": "CMAKE_PREFIX_PATH",
                    "value": "/home/ros/a_ws/install/roboclaw_driver:/opt/ros/jazzy/opt/gz_math_vendor:/opt/ros/jazzy/opt/gz_utils_vendor:/opt/ros/jazzy/opt/gz_cmake_vendor"
                },
                {
                    "name": "COLCON_PREFIX_PATH",
                    "value": "/home/ros/a_ws/install:/home/ros/sigyn_testicle_twister_ws/install"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "/opt/ros/jazzy/opt/rviz_ogre_vendor/lib:/opt/ros/jazzy/lib/aarch64-linux-gnu:/opt/ros/jazzy/opt/gz_math_vendor/lib:/opt/ros/jazzy/opt/gz_utils_vendor/lib:/opt/ros/jazzy/opt/gz_cmake_vendor/lib:/opt/ros/jazzy/lib"
                },
                {
                    "name": "PATH",
                    "value": "/opt/ros/jazzy/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin"
                },
                {
                    "name": "PYTHONPATH",
                    "value": "/home/ros/sigyn_testicle_twister_ws/build/sigyn_testicle_twister:/home/ros/sigyn_testicle_twister_ws/install/sigyn_testicle_twister/lib/python3.12/site-packages:/opt/ros/jazzy/lib/python3.12/site-packages"
                },
                {
                    "name": "RCUTILS_LOGGING_SEVERITY_THRESHOLD",
                    "value": "DEBUG"
                },
                {
                    "name": "RCUTILS_LOGGING_USE_STDOUT",
                    "value": "1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set source directories",
                    "text": "directory /home/ros/a_ws/src/roboclaw_driver/src",
                    "ignoreFailures": true
                },
                {
                    "description": "Set source directories for headers",
                    "text": "directory /home/ros/a_ws/src/roboclaw_driver/include",
                    "ignoreFailures": true
                },
                {
                    "description": "Set workspace root",
                    "text": "directory /home/ros/a_ws",
                    "ignoreFailures": true
                },
                {
                    "description": "Set compilation directory",
                    "text": "directory /home/ros/a_ws/build/roboclaw_driver",
                    "ignoreFailures": true
                },
                {
                    "description": "Show source mapping",
                    "text": "show directories",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/home/ros/a_ws/build/roboclaw_driver": "/home/ros/a_ws/src/roboclaw_driver",
                "/home/ros/a_ws/src": "/home/ros/a_ws/src"
            },
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": true
            }
        },
        {
            "name": "Attach to RoboClaw Driver",
            "type": "cppdbg",
            "request": "attach",
            "program": "/home/ros/a_ws/install/roboclaw_driver/lib/roboclaw_driver/roboclaw_driver_node",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set source directories",
                    "text": "directory /home/ros/a_ws/src/roboclaw_driver/src",
                    "ignoreFailures": true
                },
                {
                    "description": "Set source directories for headers",
                    "text": "directory /home/ros/a_ws/src/roboclaw_driver/include",
                    "ignoreFailures": true
                },
                {
                    "description": "Set workspace root",
                    "text": "directory /home/ros/a_ws",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/home/ros/a_ws/build/roboclaw_driver": "/home/ros/a_ws/src/roboclaw_driver",
                "/home/ros/a_ws/src": "/home/ros/a_ws/src"
            },
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
